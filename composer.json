{
    "name": "cvo-technologies/croogo-webshop-plugin",
    "type": "cakephp-plugin",
    "require": {
        "croogo/core": "3.0.*@dev",
        "croogo/nodes": "3.0.*@dev",
        "cvo-technologies/croogo-nodes-datasource-plugin": "3.0.*@dev",
        "cvo-technologies/croogo-webshop-payments-plugin": "3.0.*@dev",
        "cvo-technologies/webshop-taxes": "3.0.*@dev",
        "cvo-technologies/socialseo": "3.0.*@dev",
        "cvo-technologies/macro": "dev-master"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "dev-master",
        "phpunit/phpunit": "4.1.*",
        "friendsofcake/cakephp-test-utilities": "dev-master"
    },
    "replace": {
        "cvo-technologies/webshop-invoices": "3.0.*@dev"
    },
    "authors": [
        {
            "name": "Marlin Cremers",
            "email": "m.cremers@cvo-technologies.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Webshop\\": "src",
            "Webshop\\Invoices\\": "plugins/Invoices/src",
            "Webshop\\Orders\\": "plugins/Orders/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webshop\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev"
}
